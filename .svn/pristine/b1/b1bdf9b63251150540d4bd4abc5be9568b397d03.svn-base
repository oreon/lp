    <script src="/js/forms/newapp.js"></script>
    <link rel="stylesheet" href="/css/form/new-app.css">


    <script th:inline="javascript">
        /*<![CDATA[*/
        var _csrf_token = /*[[${_csrf.token}]]*/ '';
        var _csrf_param_name = /*[[${_csrf.parameterName}]]*/ '';
        /*]]>*/
    </script>


    <script th:if="${(#httpServletRequest.getParameter('confirmed') != null) and (#httpServletRequest.getParameter('confirmed') == 'true')}" th:inline="javascript">
        $(document).ready(function(){
            $('#newApplicationForm').hide();
            $('#confirmation').show();
            $('#helpContainer').hide();
            $('#formIntro').show();

            $('#submittedDate').html();
            $('#efilingInfo').show();
        });
    </script>

    <script>
        $(document).ready(function(){
            $('#_csrf_param_name').attr('name', _csrf_param_name);
            $('#_csrf_param_name').val(_csrf_token);
        });
    </script>

    <form name="UserDetailsForm" id="UserDetailsForm" method="post">
        <input type="hidden" name="userName" id="UserName" th:value="${user.getLSONumber()}" />
        <input type="hidden" name="formName" value="NewApplicationForm" />
    </form>

    <div class="single-pro-review-area mt-t-30 mg-b-15" id="formIntro">
